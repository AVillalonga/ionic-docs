{
  "name": "ionic-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run generate-markdown; scripts/i18n.sh; if [ -n \"$CROWDIN_PERSONAL_TOKEN\" ]; then npm run crowdin:sync; fi",
    "build": "npm run build:${VERCEL_ENV:-preview}",
    "build:preview": "docusaurus build --locale en",
    "build:production": "docusaurus build",
    "clear": "docusaurus clear",
    "crowdin:sync": "docusaurus write-translations && crowdin upload && crowdin download",
    "deploy": "docusaurus deploy",
    "docusaurus": "docusaurus",
    "serve": "docusaurus serve",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle --danger"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.16",
    "@ionic-internal/docusaurus-preset": "^0.0.1",
    "clsx": "^1.1.1",
    "concurrently": "^6.2.0",
    "crowdin": "^3.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "remark-html": "^13.0.1",
    "remark-parse": "^8.0.3",
    "unified": "^9.2.1"
  },
  "devDependencies": {
    "@ionic/prettier-config": "^2.0.0",
    "@tsconfig/docusaurus": "^1.0.4",
    "@types/react": "^17.0.37",
    "prettier": "^2.5.0",
    "typescript": "^4.5.2"
  }
}
